//}

// ForEach Loop
num.forEach((element) => {
  console.log(element * element)
  
})

// Array.from
let name = "Saif"
let arr = Array.from(name)
console.log(arr)

let n ="kaif"
let a = Array.from(n)
console.log(a)

//for.....of

for(let item of num){
  console.log(item)
}

// for.... in
for(let i in num){
  console.log(i)
}

